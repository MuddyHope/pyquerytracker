[MASTER]
ignore=examples

[MESSAGES CONTROL]
disable=
    missing-module-docstring,
    missing-function-docstring,
    missing-class-docstring,
    too-few-public-methods,
    fixme,
    duplicate-code,
    logging-fstring-interpolation

[REPORTS]
reports=no

[FORMAT]
max-line-length=88

[DESIGN]
max-args=7
max-locals=20

[LOGGING]
logging-format-style=old
